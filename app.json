{
  "expo": {
    "name": "CarePoint",
    "slug": "CarePoint",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./images/logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./images/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.wells3.CarePoint"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./images/logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./images/logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "react-native-health-connect"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "90a97bf1-5260-49eb-b525-b74df6eb0191"
      },

      "expo": {
        "android": {
          "minSdkVersion": 26
        }
      }
    }
  }
}
